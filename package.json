{
  "name": "html5-cli",
  "version": "2.0.1",
  "description": "generate an html5 project for mobile devices",
  "main": "README.md",
  "bin": {
    "html5": "bin/html5",
    "html5-init": "bin/html5-init",
    "h5": "bin/html5",
    "h5-init": "bin/html5-init"
  },
  "files": [
    "bin",
    "lib",
    "LICENSE",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "scripts": {
    "ready": "npm install",
    "debugger-h5": "node ./bin/html5 init temp",
    "debugger-h5-init": "node ./bin/html5-init temp",
    "template:ready": "cd template && npm run ready",
    "template:dev": "cd template && npm run dev",
    "template:lint": "cd template && npm run lint",
    "template:lint:fix": "cd template && npm run lint:fix",
    "template:build": "cd template && npm run build",
    "release": "bash bash/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yakima-Teng/html5-cli.git"
  },
  "keywords": [
    "html5-cli",
    "html5",
    "h5-cli",
    "h5"
  ],
  "author": "Yakima (yakima.public@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yakima-Teng/html5-cli/issues"
  },
  "homepage": "https://github.com/Yakima-Teng/html5-cli#readme",
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "2.19.0",
    "download-git-repo": "1.1.0",
    "fs-extra": "7.0.1",
    "ora": "3.1.0",
    "util": "0.11.1"
  },
  "engines": {
    "node": "> 8"
  }
}
